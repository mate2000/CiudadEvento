                android:inputType="textPersonName"
                tools:layout_editor_absoluteX="148dp"
                tools:layout_editor_absoluteY="122dp" />

            <EditText
                android:id="@+id/txtFechaFinalizacion"
                android:layout_width="258dp"
                android:layout_height="wrap_content"
                android:layout_x="145dp"
                android:layout_y="227dp"
                android:ems="10"
                android:inputType="date"
                tools:layout_editor_absoluteX="144dp"
                tools:layout_editor_absoluteY="163dp"
                />

            <EditText
                android:id="@+id/txtFechaInicio"
                android:layout_width="253dp"
                android:layout_height="wrap_content"
                android:layout_x="140dp"
                android:layout_y="169dp"
                android:ems="10"
                android:inputType="date"
                tools:layout_editor_absoluteX="144dp"
                tools:layout_editor_absoluteY="163dp" />

            <EditText
                android:id="@+id/txtHoraFinalizacion"
                android:layout_width="237dp"
                android:layout_height="wrap_content"
                android:layout_x="148dp"
                android:layout_y="346dp"
                android:ems="10"
                android:inputType="time"
                tools:layout_editor_absoluteX="154dp"
                tools:layout_editor_absoluteY="271dp" />

            <EditText
                android:id="@+id/txtHorainicio"
                android:layout_width="237dp"
                android:layout_height="wrap_content"
                android:layout_x="148dp"
                android:layout_y="287dp"
                android:ems="10"
                android:inputType="time"
                tools:layout_editor_absoluteX="154dp"
                tools:layout_editor_absoluteY="271dp" />

            <EditText
                android:id="@+id/txtLugar"
                android:layout_width="269dp"
                android:layout_height="wrap_content"
                android:layout_x="86dp"
                android:layout_y="417dp"
                android:ems="10"
                android:gravity="start|top"
                android:inputType="textMultiLine"
                tools:layout_editor_absoluteX="156dp"
                tools:layout_editor_absoluteY="365dp" />

            <EditText
                android:id="@+id/txtDescripcion"
                android:layout_width="382dp"
                android:layout_height="63dp"
                android:layout_x="16dp"
                android:layout_y="658dp"
                android:ems="10"
                android:gravity="start|top"
                android:inputType="textMultiLine"
                tools:layout_editor_absoluteX="26dp"
                tools:layout_editor_absoluteY="465dp" />

            <Button
                android:id="@+id/btnEnviar"
                android:layout_width="145dp"
                android:layout_height="wrap_content"
                android:layout_x="126dp"
                android:layout_y="762dp"
                android:text="Enviar"
                tools:layout_editor_absoluteX="69dp"
                tools:layout_editor_absoluteY="44dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="13dp"
                android:layout_y="509dp"
                android:text="Solicitar Financiacion"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="472dp"
                android:textColor="@android:color/black"/>

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="18dp"
                android:layout_y="562dp"
                android:text="Solicitar Publicidad"
                tools:layout_editor_absoluteX="5dp"
                tools:layout_editor_absoluteY="559dp"
                android:textColor="@android:color/black"/>

            <CheckBox
                android:id="@+id/cbfinanciacionEvento"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="181dp"
                android:layout_y="500dp"
                android:text="Socilitar"
                tools:layout_editor_absoluteX="201dp"
                tools:layout_editor_absoluteY="511dp" />

            <CheckBox
                android:id="@+id/cbPublicidadEvento"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="182dp"
                android:layout_y="553dp"
                android:text="Solicitar"
                tools:layout_editor_absoluteX="185dp"
                tools:layout_editor_absoluteY="553dp" />
        </AbsoluteLayout>

</ScrollView>