<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ServiceProvider.MainActivityPostulateEvent">

        <AbsoluteLayout
            android:layout_width="825dp"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="17dp"
                android:layout_y="632dp"
                android:text="Descripcion"
                android:textColor="@android:color/black"
                tools:layout_editor_absoluteX="12dp"
                tools:layout_editor_absoluteY="177dp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="8dp"
                android:layout_y="370dp"
                android:text="Hora de finalizacion"
                android:textColor="@android:color/black"
                tools:layout_editor_absoluteX="12dp"
                tools:layout_editor_absoluteY="177dp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="56dp"
                android:layout_height="wrap_content"
                android:layout_x="13dp"
                android:layout_y="438dp"
                android:text="Lugar"
                android:textColor="@android:color/black"
                tools:layout_editor_absoluteX="12dp"
                tools:layout_editor_absoluteY="177dp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="11dp"
                android:layout_y="299dp"
                android:text="Hora de inicio"
                android:textColor="@android:color/black"
                tools:layout_editor_absoluteX="12dp"
                tools:layout_editor_absoluteY="177dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="120dp"
                android:layout_y="46dp"
                android:text="Postular nuevo evento"
                android:textColor="@android:color/black"
                android:textSize="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_x="15dp"
                android:layout_y="121dp"
                android:text="Nombre Evento"
                android:textColor="@android:color/black"
