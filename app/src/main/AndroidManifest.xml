        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TourismSecretary.MainPostulatedEventPanel"></activity>
        <activity android:name=".ServiceProvider.MainPanelServiceProvider" />
        <activity android:name=".TourismSecretary.MainPanelTourismSecretary"></activity>
        <activity android:name=".LoginActivities.MainLoginTS" />
        <activity android:name=".ServiceProvider.MainActivityPostulateEvent" />
        <activity android:name=".ServiceProvider.MainPrincipalScreenSP" />
        <activity android:name=".LoginActivities.MainLoginSP" />
        <activity android:name=".RegisterActivities.MainRegisterSP" />
        <activity android:name=".MainPrincipalScreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>